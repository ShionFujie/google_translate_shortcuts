{
  "name": "Google Translate Shortcuts",
  "version": "0.0.1",
  "description": "Additional shortcuts for Google Translate",
  "permissions": [],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://translate.google.com/*"],
      "js": ["js/content.js", "lib/jquery-3.4.1.min.js"]
    }
  ]
}
